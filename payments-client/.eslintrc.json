{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "indent": "off",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": "off",
    "no-unused-vars": "off",
    "arrow-parens": "off",
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
    "radix": "off"
  }
}
